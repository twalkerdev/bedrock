{
	"name": "@repo/bedrock",
	"version": "0.1.0",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@10.13.1",
	"scripts": {
		"lint": "pnpx eslint",
		"lint:staged": "lint-staged",
		"clean": "pnpm -r run clean",
		"reset": "pnpm run clean && rm -rf node_modules",
		"format": "prettier --write .",
		"format:staged": "pretty-quick --staged",
		"dev": "tsc --watch",
		"build": "vite build",
		"test": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage"
	},
	"exports": {
		".": {
			"import": "./dist/bedrock.esm.js",
			"require": "./dist/bedrock.umd.cjs"
		},
		"./linked-list": {
			"types": "./src/linked-list/linked-list.ts",
			"default": "./dist/linked-list/linked-list.js"
		},
		"./queue": {
			"types": "./src/queue/queue.ts",
			"default": "./dist/queue/queue.js"
		}
	},
	"devDependencies": {
		"@chance/eslint": "^1.1.0",
		"@eslint/js": "^9.32.0",
		"@lib/eslint-config": "workspace:*",
		"@lib/typescript-config": "workspace:*",
		"@vitest/coverage-istanbul": "3.2.4",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.6.2",
		"pretty-quick": "^4.2.2",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@lib/eslint-config": "workspace:*"
	}
}
