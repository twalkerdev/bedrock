{
  "name": "@bedrock/common",
  "version": "0.1.0",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "import": {
          "types": "./dist/index.d.mts",
          "default": "./dist/index.mjs"
        },
        "require": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        }
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "lint": "eslint --max-warnings 0 src",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "build": "tsup"
  },
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@lib/typescript-config": "workspace:*",
    "@lib/eslint-config": "workspace:*",
    "eslint": "^9.32.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  }
}
