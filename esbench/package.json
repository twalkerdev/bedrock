{
	"name": "@repo/benchmark",
	"private": true,
	"version": "0.0.0",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@10.13.1",
	"scripts": {
		"build:": "tsc",
		"lint": "pnpm -r run lint",
		"lint:staged": "lint-staged",
		"clean": "pnpm -r run clean",
		"reset": "pnpm run clean && rm -rf node_modules",
		"format": "prettier --write .",
		"format:staged": "pretty-quick --staged",
		"benchmark": "esbench --config esbench.config.js"
	},
	"devDependencies": {
		"@chance/eslint": "^1.1.0",
		"@eslint/js": "^9.32.0",
		"esbench": "^0.8.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.6.2",
		"pretty-quick": "^4.2.2",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.38.0"
	},
	"dependencies": {
		"@repo/bedrock": "workspace:*",
		"@lib/eslint-config": "workspace:*"
	}
}
