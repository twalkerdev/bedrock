{
	"name": "@lib/eslint-config",
	"version": "0.0.0",
	"private": true,
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./index.ts",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "tsc",
		"lint": "pnpm eslint . --ext .ts",
		"lint:staged": "lint-staged",
		"clean": "pnpm -r run clean",
		"reset": "pnpm run clean && rm -rf node_modules",
		"format": "prettier --write .",
		"format:staged": "pretty-quick --staged"
	},
	"devDependencies": {
		"@chance/eslint": "^1.1.0",
		"@eslint/js": "^9.32.0",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "^3.6.2",
		"pretty-quick": "^4.2.2",
		"typescript-eslint": "^8.38.0"
	},
	"peerDependencies": {
		"eslint": ">=9.32.0"
	}
}
